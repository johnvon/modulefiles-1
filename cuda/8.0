#%Module 1.0
conflict	 cuda
setenv		 CUDAROOT /usr/local/cuda-8.0
setenv		 CUDA_INSTALL_PATH /usr/local/cuda-8.0
prepend-path	 PATH /usr/local/cuda-8.0/bin
prepend-path	 LD_LIBRARY_PATH /usr/local/cuda-8.0/lib
prepend-path	 LD_LIBRARY_PATH /usr/local/cuda-8.0/lib64
prepend-path	 LD_RUN_PATH /usr/local/cuda-8.0/lib
prepend-path	 LD_RUN_PATH /usr/local/cuda-8.0/lib64
prepend-path	 -d " " LDFLAGS -L/usr/local/cuda-8.0/lib
prepend-path	 -d " " LDFLAGS -L/usr/local/cuda-8.0/lib64
prepend-path	 C_INCLUDE_PATH /usr/local/cuda-8.0/include
prepend-path	 CPLUS_INCLUDE_PATH /usr/local/cuda-8.0/include
