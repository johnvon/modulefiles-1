#%Module 1.0

module-whatis	 Provides GNU 5.3.0 Compilers
conflict	 compiler
setenv		 CC gcc
setenv		 CXX g++
setenv		 FC gfortran
setenv		 F90 gfortran
setenv		 F77 gfortran
setenv		 COMPILERVERSION gcc-5.3.0
prepend-path	 PATH /opt/scc/gcc/5.3.0/bin
prepend-path	 LD_LIBRARY_PATH /opt/scc/gcc/5.3.0/lib64
prepend-path	 LD_RUN_PATH /opt/scc/gcc/5.3.0/lib64
prepend-path	 -d " " LDFLAGS -L/opt/scc/gcc/5.3.0/lib64
prepend-path	 MANPATH /opt/scc/gcc/5.3.0/share/man
